<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Turnos" table="turnos">
    <id name="intTurnoId" type="integer" column="int_turno_id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="varTurnoHorainicio" type="string" column="var_turno_horainicio" length="10" nullable="true"/>
    <field name="varTurnoHorafin" type="string" column="var_turno_horafin" length="10" nullable="true"/>
    <field name="intTurnoDia" type="integer" column="int_turno_dia" nullable="true"/>
    <many-to-one field="servicio" target-entity="Servicios">
      <join-columns>
        <join-column name="servicio_id" referenced-column-name="int_servicio_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
